<h1> Registeration Page</h1>

<%= form_with model: @user, url:'/create_user' do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </div>
  
  <div>
    <%= form.submit "Submit" %>
  </div>
<% end %>
<h1>your at create post page </h1>
<h1>New Article</h1>

<%= form_with model: @article, url: '/create_post' do |form| %>
  <div>
    <%= form.label :tittle %><br>
    <%= form.text_field :tittle %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>
  
  <div>
    <%= form.submit %>
  </div>
<% end %>